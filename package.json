{
  "name": "express-typescript-mongoose-starter",
  "version": "1.0.0",
  "description": "A starter for Node JS, Express, Typescript, Mongoose aplication",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run watch-ts\"",
    "serve": "npm run build-ts & npm run start",
    "build-ts": "tsc -p src",
    "watch-ts": "tsc-watch -w -p src --onSuccess \"npm run start-dev\"",
    "start": "pm2 start dist/server.js",
    "start-dev": "NODE_ENV=development nodemon dist/server.js"
  },
  "keywords": [
    "Node",
    "JS",
    "Express",
    "Mongo",
    "Mongoose",
    "Typescript"
  ],
  "author": "Manish Kumar Jha",
  "license": "ISC",
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/request": "^2.48.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "busboy": "^0.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^4.3.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "express-fileupload": "^1.1.1-alpha.1",
    "mongoose": "^5.7.8",
    "morgan": "^1.8.2",
    "node-cron": "^2.0.3",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^4.7.19",
    "@types/node": "^8.0.17",
    "chai": "^4.2.0",
    "concurrently": "^3.5.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "ts-node": "^8.0.2",
    "tsc-watch": "^1.0.7",
    "typescript": "^2.4.2"
  }
}